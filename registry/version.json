{
    "protocols": [
        "4.0",
        "5.1"
    ],
    "os": "${OS}",
    "arch": "${ARCH}",
    "filename": "${BASE_NAME}_${VERSION}_${OS}_amd64.zip",
    "download_url": "https://${HOSTNAME}/${BASE_NAME}/${VERSION}/${BASE_NAME}_${VERSION}_${OS}_${ARCH}.zip",
    "shasums_url": "https://${HOSTNAME}/${BASE_NAME}/${VERSION}/${BASE_NAME}_${VERSION}_SHA256SUMS",
    "shasums_signature_url": "https://${HOSTNAME}/${BASE_NAME}/${VERSION}/${BASE_NAME}_${VERSION}_SHA256SUMS.sig",
    "shasum": "${SHASUM}",
    "signing_keys": {
        "gpg_public_keys": [
            {
                "key_id": "${GPG_KEY_ID}",
                "ascii_armor": "${GPG_ASCII_ARMOR}",
                "trust_signature": "",
                "source": "Spacelift",
                "source_url": "https://docs.spacelift.io/product/security"
            }
        ]
    }
}
